class Solution:
    def maxProduct(self, words):
        m=[]
        for w in words:
            mask=0
            for c in w:
                mask|=1<<(ord(c)-97)
            m.append((mask,len(w)))
        ans=0
        for i in range(len(m)):
            for j in range(i+1,len(m)):
                if m[i][0]&m[j][0]==0:
                    ans=max(ans,m[i][1]*m[j][1])
        return ans
